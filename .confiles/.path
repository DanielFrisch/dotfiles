# default path
PATH=/usr/local/sbin:usr/local/lib:/usr/local/mysql/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/local/bin


# nvm
PATH=$PATH:$(brew --prefix nvm)/nvm.sh

# node
PATH=$PATH:$(npm config get prefix)/bin

# homebrew
PATH=$(brew --prefix)/bin:$(brew --prefix)/sbin:$(brew --prefix coreutils)/libexec/gnubin:$PATH

# Add `~/bin` to the `$PATH`
PATH=$HOME/bin:$PATH

# If there is bin folder in the current location, it takes preference
# Usefull for binstubs in rails
PATH=./bin:$PATH

export PATH
